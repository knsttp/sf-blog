<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Symfony Hotel Demo{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>
    {% block body %}
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <a target="_blank" href="https://www.benedict.cloud/blog/2019/06/17/symfony-4-a-crud-tutorial-second-edition/">
                        symfony-4-a-crud-tutorial-second-edition
                    </a>
                    <h1 class="pb-2 my-4 border-bottom">{% block header %}{% endblock %}</h1>
                    {% block content %}{% endblock %}
                </div>
            </div>
        </div>
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</body>
</html>
